{
	"name": "bangumiopen",
	"author": "sdttttt",
	"repository": "https://github.com/sdttttt/vscode-Bangumi",
	"displayName": "Bangumi Open",
	"description": "The favorite of otakuo friends.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.openBangumi",
		"onCommand:extension.nextPage",
		"onCommand:extension.backPage"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openBangumi",
				"title": "Open Bangumi"
			},
			{
				"command": "extension.nextPage",
				"title": "Next Page"
			},
			{
				"command": "extension.backPage",
				"title": "Back Page"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.8",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"chai": "^4.2.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@types/axios": "^0.14.0",
		"axios": "^0.19.2"
	}
}
